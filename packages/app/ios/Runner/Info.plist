    <string>6.0</string>
    <key>CFBundleName</key>
    <string>keychat</string>
    <key>CFBundlePackageType</key>
    <string>APPL</string>
    <key>CFBundleShortVersionString</key>
    <string>$(FLUTTER_BUILD_NAME)</string>
    <key>CFBundleSignature</key>
    <string>????</string>
    <key>NSFaceIDUsageDescription</key>
    <string>For your privacy protection</string>
    <key>CFBundleURLTypes</key>
    <array>
      <dict>
        <key>CFBundleTypeRole</key>
        <string>Editor</string>
        <key>CFBundleURLSchemes</key>
        <array>
          <string>ShareMedia-$(PRODUCT_BUNDLE_IDENTIFIER)</string>
        </array>
      </dict>
      <dict>
        <key>CFBundleTypeRole</key>
        <string>Viewer</string>
        <key>CFBundleURLIconFile</key>
        <string>https://www.keychat.io/assets/img/logo.png</string>
        <key>CFBundleURLName</key>
        <string>com.keychat.test</string>
        <key>CFBundleURLSchemes</key>
        <array>
          <string>keychat</string>
          <string>lnurlp</string>
          <string>lightning</string>
          <string>nostr</string>
          <string>cashu</string>
          <string>bitcoin</string>
        </array>
      </dict>
    </array>
    <key>CFBundleVersion</key>
    <string>$(FLUTTER_BUILD_NUMBER)</string>
    <key>FirebaseMessagingAutoInitEnabled</key>
    <true/>
    <key>FirebaseAppDelegateProxyEnabled</key>
    <true/>
    <key>FlutterDeepLinkingEnabled</key>
    <false/>
    <key>ITSAppUsesNonExemptEncryption</key>
    <false/>
    <key>LSApplicationQueriesSchemes</key>
    <array>
      <string>lightning</string>
      <string>lnurlp</string>
    </array>
    <key>LSRequiresIPhoneOS</key>
    <true/>
    <key>NSUserNotificationsUsageDescription</key>
    <string>This app needs notification permission to send you local notifications</string>
    <key>NSAppTransportSecurity</key>
    <dict>
      <key>NSAllowsArbitraryLoads</key>
      <true/>
      <key>NSAllowsArbitraryLoadsInWebContent</key>
      <true/>
      <key>NSAllowsLocalNetworking</key>
      <true/>
    </dict>
    <key>NSCameraUsageDescription</key>
    <string>Can we access your camera in order to scan barcodes?</string>
    <key>NSMicrophoneUsageDescription</key>
    <string>Enable this to calling</string>
    <key>NSPhotoLibraryAddUsageDescription</key>
    <string>Enable this to save file to gallery</string>
